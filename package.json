{
  "name":        "zodiac",
  "version":     "0.1.10",
  "description": "Reactive web component microframework based on Tracker",
  "keywords": [
    "reactive",
    "templates",
    "web",
    "components",
    "views",
    "frp",
    "spa"
  ],
  "homepage": "https://github.com/jbe/zodiac",
  "bugs": {
    "url": "https://github.com/jbe/zodiac/issues"
  },
  "license": "MIT",
  "author":  "Jostein B. Eliassen (http://jostein.be)",
  "main":    "dist/zodiac.js",
  "repository": "jbe/zodiac.git",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel ./src --out-dir dist",
    "dev": "webpack-dev-server",
    "test": "mocha",
    "phantomtest": "npm run build && node_modules/.bin/mocha-phantomjs ./test/index.html"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-es2016": "*",
    "babel-preset-stage-0": "*",
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-polyfill": "*",
    "http-server": "*",
    "webpack": "*",
    "webpack-dev-server": "*",

    "mocha":           ">= 2.4",
    "mocha-phantomjs": "*",
    "should":          "*"
  }
}
